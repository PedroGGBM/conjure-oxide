language Essence 1.3

find x : int(0..1000)
such that x = sum([ i + j
                  | i : int(1..3)
                  , j <- [ k * 2 | k : int(1..4) ]
                  , j < 7
                  ])

find y : int(0..1000)
such that y = sum([ i + (k * 2)
                | i : int(1..3)
                , k : int(1..4)
                , (k * 2) < 7
                ])
